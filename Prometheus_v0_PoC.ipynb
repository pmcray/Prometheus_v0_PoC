    "os.makedirs('toy_problem', exist_ok=True)\n",
    "\n",
    "inefficient_sort_code = \"\"\"\n",
    "def inefficient_sort(data):\n",
    "    \\\"\\\"\\\"\n",
    "    An intentionally inefficient sorting algorithm (bubble sort).\n",
    "    \\\"\\\"\\\"\n",
    "    n = len(data)\n",
    "    for i in range(n):\n",
    "        for j in range(0, n-i-1):\n",
    "            if data[j] > data[j+1]:\n",
    "                data[j], data[j+1] = data[j+1], data[j]\n",
    "    return data\n",
    "\"\"\"\n",
    "\n",
    "test_code = \"\"\"\n",
    "import pytest\n",
    "from inefficient_sort import inefficient_sort\n",
